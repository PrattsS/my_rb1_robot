<?xml version="1.0" ?>
<robot name="my_rb1_robot">
    <!-- Link - base_link -->
    <link name="base_link">
        <inertial>
            <mass value="25" />
            <origin xyz="0 0 0.15" rpy="0 0 0" /> <!-- Center of mass remains at half the height -->
            <inertia ixx="2.8646" ixy="0" ixz="0" iyy="2.8646" iyz="0" izz="0.78125" />
        </inertial>

        <collision>
            <geometry>
                <cylinder radius="0.25" length="0.30" />
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius="0.25" length="0.30" />
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 1" />
            </material>
        </visual>
    </link>

    <!-- Continuous Joint - chassis / right wheel -->
    <joint name="joint_base_link_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 -0.2 -0.15" />
        <child link="right_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Continuous Joint - chassis / left wheel -->
    <joint name="joint_base_link_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.2 -0.15" />
        <child link="left_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    
    <!-- Continuous Joint - chassis / front caster wheel -->
    <joint name="joint_base_link_front_caster_wheel" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0 -0.15" />
        <child link="front_caster_wheel" />
        <parent link="base_link" />
        <!--<axis rpy="0 0 0" xyz="0 0 1" />
        <limit effort="10000" velocity="1000" />-->
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Continuous Joint - chassis / back caster wheel -->
    <joint name="joint_base_link_back_caster_wheel" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0 -0.15" />
        <child link="back_caster_wheel" />
        <parent link="base_link" />
        <!--<axis rpy="0 0 0" xyz="0 0 1" />
        <limit effort="10000" velocity="1000" />-->
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Fixed Joint - chassis / base footprint -->
    <joint name="joint_base_link_base_footprint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 -0.15" />
        <child link="base_footprint" />
        <parent link="base_link" />
        <!--<axis rpy="0 0 0" xyz="0 0 1" />
        <limit effort="10000" velocity="1000" />-->
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Fixed Joint - chassis / front laser -->
    <joint name="joint_base_link_front_laser" type="fixed">
        <origin rpy="3.1416 0 0" xyz="0.25 0 0.075" />
        <child link="front_laser" />
        <parent link="base_link" />
        <!--<axis rpy="0 0 0" xyz="1 0 0" />
        <limit effort="10000" velocity="1000" />-->
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Joint - chassis / right wheel -->
    <link name="right_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="2.8646" ixy="0" ixz="0" iyy="2.8646" iyz="0" izz="0.78125" />
        </inertial>

        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0"  />
            <geometry>
                <cylinder radius="0.025" length="0.025" />
            </geometry>
        </collision>

        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"  />
            <geometry>
                <cylinder radius="0.025" length="0.025" />
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - chassis / left wheel -->
    <link name="left_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="2.8646" ixy="0" ixz="0" iyy="2.8646" iyz="0" izz="0.78125" />
        </inertial>

        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0"  />
            <geometry>
                <cylinder radius="0.025" length="0.025" />
            </geometry>
        </collision>

        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"  />
            <geometry>
                <cylinder radius="0.025" length="0.025" />
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - chassis / front caster wheel -->
    <link name="front_caster_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025"/>
        </inertial>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"  />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"  />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="White">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - chassis / back caster wheel -->
    <link name="back_caster_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025"/>
        </inertial>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"  />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"  />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="White">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>

    <!-- Link - base_footprint -->
    <link name="base_footprint">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="2.8646" ixy="0" ixz="0" iyy="2.8646" iyz="0" izz="0.78125" />
        </inertial>

        <!--<collision>
            <geometry>
                <cylinder radius="0.25" length="0.30" />
            </geometry>
        </collision>-->

        <!--<visual>
            <geometry>
                <cylinder radius="0.25" length="0.30" />
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 1" />
            </material>
        </visual>-->
    </link>

    <!-- Link - front_link -->
    <link name="front_laser">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="2.8646" ixy="0" ixz="0" iyy="2.8646" iyz="0" izz="0.78125" />
        </inertial>

        <!--<collision>
            <geometry>
                <cylinder radius="0.25" length="0.30" />
            </geometry>
        </collision>-->

        <!--<visual>
            <geometry>
                <cylinder radius="0.25" length="0.30" />
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 1" />
            </material>
        </visual>-->
    </link>



</robot>
